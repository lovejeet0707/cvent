    @future
    public static void GDPRUser(Set<Id> GDPRIdSet)
    {
        List<User> UserObjList = new List<User>();
        User userObj = new User();
        For(GDPR_Central_Repository__c GDPR : [SELECT Id,Id__c,Firstname__c,Email__c,LastName__c,MobilePhone__c,Phone__c,Name__c  FROM GDPR_Central_Repository__c WHERE Id IN : GDPRIdSet])
        {
            userObj = new User(Id = GDPR.Id__c);
            userObj.FirstName = GDPR.Firstname__c;
            UserObj.Email = GDPR.Email__c;
            UserObj.Is_GDPR__c = false;//UsertactIdMap.get(UserObj.Id).Is_GDPR__c;
            UserObj.LastName = GDPR.LastName__c;
            UserObj.MobilePhone = GDPR.MobilePhone__c;
            UserObj.Phone= GDPR.Phone__c;
            UserObj.UserName= GDPR.Name__c ;
            UserObjList.add(UserObj);
        }
        If(UserObjList.size()>0){
            update UserObjList;
        }
    } 
