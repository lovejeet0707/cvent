    <validationRules>
        <fullName>IMP_Survey_is_locked</fullName>
        <active>true</active>
        <description>Case# - 10140322</description>
        <errorConditionFormula>AND( 
ISPICKVAL(Opportunity__r.StageName,"Closed Won"), 
NOT(CONTAINS($User.Profile_Name__c, "Contract")), 
NOT(CONTAINS($User.Username,"deployment")) 
)</errorConditionFormula>
        <errorDisplayField>X2019_Mid_Year_Promo_influence_on_Deal__c</errorDisplayField>
        <errorMessage>You can not edit Implementation Survey now. Please contact ARHelp@cvent.com.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandatory_Give_Specific_Details</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Upgrade_account_from_Renewal_Desk_to_AM__c,"Yes"), 
ISBLANK(Give_Specific_Details__c), 
NOT(CONTAINS($User.Username,"deploymentuser@cvent.com")) 
)</errorConditionFormula>
        <errorMessage>Give specific detail over how this account will grow and why it requires a traditional AM</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mandatory_fields_for_CC_Products</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
RecordTypeId='0121N000001AQ3E', /* Event Cloud */ 
Quote__r.Count_of_CrowdCompass_Products__c &gt; 0, 
OR( 
ISBLANK(First_Mobile_Event_Date__c), 
ISBLANK(First_Mobile_Event_Launch_Date__c), 
ISBLANK(How_Many_Mobile_Apps_Purchased_in_Year_1__c), 
ISPICKVAL(Rush_Request__c,""), 
ISPICKVAL(Mobile_Delivery_Date__c,'') 
), 

OR( 
$UserRole.Id='00E00000006omm8', /* Direct Sales (Event) */ 
$UserRole.Id='00E00000006uNkm', /* Direct Sales (CrowdCompass) */ 
$UserRole.Id='00E00000006tgck', /* Direct Sales (Ent) */ 
$UserRole.Id='00E00000006uFmQ', /*Direct Sales (TPP) */ 
$UserRole.Id='00E00000006tiKx', /* AM (Event) */ 
$UserRole.Id='00E00000006uKyb', /* AM (Ent) */ 
$UserRole.Id='00E00000006uOXA', /*AM (TPP) */ 
$UserRole.Id='00E00000006QdYr', /*Pre Sales (Event) */ 
$UserRole.Id='00E00000006onAo', /* Pre Sales India (Event) */ 
$UserRole.Id='00E00000006uCdf', /*Pre Sales (External) */ 
$UserRole.Id='00E00000006uNkr', /* Pre Sales (CrowdCompass) */ 
$UserRole.Id='00Eo0000000GZBD', /*Pre Sales India (CrowdCompass) */ 
$UserRole.Id='00E00000006tgcp', /* Pre Sales (Ent) */ 
$UserRole.Id='00E00000006uFmL', /* Pre Sales (TPP) */ 
$UserRole.Id='00E00000006uGUr' /*Pre Sales India (TPP) */ 

), 

NOT(CONTAINS($User.Username,"deploymentuser@cvent.com")), 
$User.Validation_Bypass__c = False, 
$Setup.rh2__PS_Settings__c.rh2__Run_Validation_Rule__c /*Added to bypass rollup helper updates */ 

)</errorConditionFormula>
        <errorMessage>Please fill in all required information for Mobile Apps in the Additional Details section.</errorMessage>
    </validationRules>
