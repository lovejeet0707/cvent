    <validationRules>
        <fullName>AMex_and_non_usd_currency</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL(Type_of_Card__c,"AmEx"),
NOT(ISPICKVAL(Currency__c,"USD")),
ISPICKVAL(Flux_Approval__c,"")
)</errorConditionFormula>
        <errorMessage>Flux Approval is mandatory for Amex Card and NON USD Currency</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approval_required_for_amount</fullName>
        <active>true</active>
        <description>If amount is greater than 7500, approval is required</description>
        <errorConditionFormula>AND(
ISPICKVAL(Amountt_Approval__c,""),
Amount__c&gt;7500
)</errorConditionFormula>
        <errorDisplayField>Amountt_Approval__c</errorDisplayField>
        <errorMessage>You must select Approval</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Collection_team_status</fullName>
        <active>true</active>
        <description>Collection team can only use pending for Charge or On Hold Status</description>
        <errorConditionFormula>AND(
ISCHANGED(Status__c),
NOT(OR(ISPICKVAL(Status__c,"Pending for charge"),
ISPICKVAL(Status__c,"On Hold"),
ISPICKVAL(Status__c,"Pending for review"))),
CONTAINS($UserRole.Name,"Collection")
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>You cannot use any other status except Pending for Charge, Pending for review and On Hold</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Exp_Date_should_be_present_or_future</fullName>
        <active>true</active>
        <description>Exp Date should be always today or future date</description>
        <errorConditionFormula>Exp_Date__c &lt; TODAY()</errorConditionFormula>
        <errorDisplayField>Date_Bound__c</errorDisplayField>
        <errorMessage>Date Bound should be today's or future date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Flux_Approval_Mandatory</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Flux_Approval__c,"No")</errorConditionFormula>
        <errorDisplayField>Flux_Approval__c</errorDisplayField>
        <errorMessage>You must have Flux Approval.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Force_Charge_approval_grid_mandate</fullName>
        <active>true</active>
        <description>validation to make Force Charge approval grid mandate, when 'To be force charged' is selected in 'Action'</description>
        <errorConditionFormula>AND(
ISPICKVAL(Action__c , "To be force charged"),
ISPICKVAL(Force_Charge_Approval_Grid__c,"" )
)</errorConditionFormula>
        <errorDisplayField>Force_Charge_Approval_Grid__c</errorDisplayField>
        <errorMessage>Please select the value in "Force Charge Approval Grid" field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Negative_amount_for_to_be_refunded</fullName>
        <active>true</active>
        <description>If action is to be refunded, amount field should be less than zero</description>
        <errorConditionFormula>AND(
ISPICKVAL(Action__c,"To be refunded"),
Amount__c&gt;0.0
)</errorConditionFormula>
        <errorDisplayField>Amount__c</errorDisplayField>
        <errorMessage>For "To be refunded" action, Amount cannot be greater than 0.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_card_tpe_mandate</fullName>
        <active>true</active>
        <description>If Type of Card is selected as Other, then Other Card type field is mandatory</description>
        <errorConditionFormula>AND(
ISPICKVAL(Type_of_Card__c,"Other"),
ISBLANK(Other_Card_Type__c)
)</errorConditionFormula>
        <errorDisplayField>Other_Card_Type__c</errorDisplayField>
        <errorMessage>Please enter Other Card Type</errorMessage>
    </validationRules>
</CustomObject>
