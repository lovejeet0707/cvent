    <validationRules>
        <fullName>Added_in_data_dictionary</fullName>
        <active>true</active>
        <description>If governance type field value is "New Field", it's mandatory to choose 'Yes' in 'Added in Data Dictionary' field</description>
        <errorConditionFormula>AND(
ISPICKVAL(Governance_Type__c,"New Field"),
NOT(ISPICKVAL(Added_in_Data_Dictionary__c,"Yes"))
)</errorConditionFormula>
        <errorMessage>Kindly update the data dictionary. Link to DD:-

https://cvent.box.com/s/s6hzmv9ig0xi7ftz8zzfcre1xp0kzdot</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Approved_Date_or_Reviewer_Comment_update</fullName>
        <active>false</active>
        <errorConditionFormula>AND( OR(ISCHANGED(Approved_Date__c), ISCHANGED(Reviewer_Comments__c)),
NOT($User.Id = '00500000006opLU'),
NOT($User.Id = '00500000006s5yr'),
NOT($User.Id = '00500000007RwJC'),
NOT($User.Id = '00500000007C9CM'),
NOT($User.Id = '0051N0000055Rhc')
)</errorConditionFormula>
        <errorMessage>You are not allowed to update the Approved Date and Reviewer Comments.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Request_Status_Draft_at_Creation</fullName>
        <active>true</active>
        <description>'Request Status' should be "1-Draft in Progress" when creating a CMR.	
Case# 11540780</description>
        <errorConditionFormula>AND 
(RecordTypeId = '0121N000001fvSe', 
NOT(ISPICKVAL(Request_Status__c, "1-Draft in Progress")), 
ISNEW() 
)</errorConditionFormula>
        <errorMessage>'Request Status' should be "1-Draft in Progress" when creating a CMR.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Request_Status_Updated</fullName>
        <active>true</active>
        <errorConditionFormula>AND(RecordTypeId = '0121N000001fvSe',
ISCHANGED(Request_Status__c),
NOT(ISPICKVAL(Request_Status__c,"1-Draft in Progress")),
NOT(ISPICKVAL(Request_Status__c,"2-Ready for Review")),
$User.Id &lt;&gt; '00500000006opLU', /*Holly Simon*/
$User.Id &lt;&gt; '0051N0000055Rhc', /*Peter Mutuc*/
$User.Id &lt;&gt; '00500000007C9CM', /*David Asrani*/
$User.Id &lt;&gt; '00500000007RwJC', /*Sushil Kapoor*/
$User.Id &lt;&gt; '00500000006s5yr'  /*Alexis Petrovich*/
)</errorConditionFormula>
        <errorMessage>You are not authorized to update the Request status.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Requirement_Gathering_form_to_be_filled</fullName>
        <active>true</active>
        <errorConditionFormula>AND(RecordTypeId = '0121N000001fvSe', 
NOT( TTI_Requirement_Gathering__c &gt; 0), 
OR( 
AND(ISCHANGED(Affected_Systems__c), INCLUDES(Affected_Systems__c,"SFDC"), 
ISPICKVAL(Request_Status__c, "2-Ready for Review"), 
OR( 
ISPICKVAL(Seeking_Status__c, "2-Ready for Review"), 
ISPICKVAL(Seeking_Status__c, "3A-Conceptually Approved"), 
ISPICKVAL(Seeking_Status__c, "3C-Execution Approved")) 
), 
AND(ISCHANGED(Request_Status__c), INCLUDES(Affected_Systems__c,"SFDC"), 
ISPICKVAL(Request_Status__c, "2-Ready for Review")), 
AND( ISCHANGED(Seeking_Status__c), INCLUDES(Affected_Systems__c,"SFDC"), 
OR( 
ISPICKVAL(Seeking_Status__c, "2-Ready for Review"), 
ISPICKVAL(Seeking_Status__c, "3A-Conceptually Approved"), 
ISPICKVAL(Seeking_Status__c, "3C-Execution Approved") 
))))</errorConditionFormula>
        <errorMessage>Please fill the "CMR Requirement Gathering Form" for requests related to SFDC, before changing the status to "Ready for Review"</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Update_Case_Number</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
TEXT(Request_Status__c) = "Submitted For Approval",
ISBLANK(Case__c)
, RecordTypeId = '0121N000001AQBS'
)</errorConditionFormula>
        <errorDisplayField>Case__c</errorDisplayField>
        <errorMessage>Please update Case #</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Update_Governance_Type</fullName>
        <active>true</active>
        <description>If the request status of a governance request is being marked as 'Completed', it is mandatory to fill Governance Type field</description>
        <errorConditionFormula>AND(
ISPICKVAL(Request_Status__c,"Completed"),
 RecordTypeId = "0121N000001AQBS", /*SF Governance Request*/
ISBLANK(TEXT(Governance_Type__c))
)</errorConditionFormula>
        <errorMessage>Kindly fill value in Governance Type field</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Update_data_dictionary</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
 RecordTypeId = '0121N000001AQBS',/*SF Governance request*/
 ISPICKVAL( Request_Status__c ,'Completed'),
 ISPICKVAL(Governance_Type__c,'Field'),
 ISBLANK(Text( Added_in_Data_Dictionary__c ))
)</errorConditionFormula>
        <errorDisplayField>Added_in_Data_Dictionary__c</errorDisplayField>
        <errorMessage>Governance request of type field can be completed only if field is added in Data dictionary</errorMessage>
    </validationRules>
</CustomObject>
