        <columns>Test_Scenario__c</columns>
        <columns>Failure_Reason__c</columns>
        <columns>Comments__c</columns>
        <columns>Resolution_Owner__c</columns>
        <columns>Urgency__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>CREATED_DATE</field>
            <operation>equals</operation>
            <value>TODAY</value>
        </filters>
        <filters>
            <field>Test_Result_Status__c</field>
            <operation>equals</operation>
            <value>Failed,"Passed, with exception"</value>
        </filters>
        <label>Test Results Created Today - Failures</label>
        <language>en_US</language>
    </listViews>
    <listViews>
        <fullName>WIP</fullName>
        <columns>NAME</columns>
        <columns>Resolution_Owner__c</columns>
        <columns>Test_Scenario__c</columns>
        <columns>Urgency__c</columns>
        <columns>Test_Scenario_Status__c</columns>
        <columns>Pricing__c</columns>
        <columns>Pending_Feedback__c</columns>
        <columns>Ready_for_Retest__c</columns>
        <columns>products__c</columns>
        <columns>CREATEDBY_USER</columns>
        <columns>CREATED_DATE</columns>
        <columns>LAST_UPDATE</columns>
        <columns>Developer_Notes__c</columns>
        <filterScope>Everything</filterScope>
        <filters>
            <field>Failed__c</field>
            <operation>equals</operation>
            <value>1</value>
        </filters>
        <filters>
            <field>CREATED_DATE</field>
            <operation>greaterOrEqual</operation>
            <value>4/16/2018 12:00 AM</value>
        </filters>
        <filters>
            <field>Test_Scenario_Status__c</field>
            <operation>equals</operation>
            <value>WIP</value>
        </filters>
        <label>WIP</label>
        <language>en_US</language>
    </listViews>
    <validationRules>
        <fullName>Comments_Required_if_Blocked</fullName>
        <active>true</active>
        <description>Comments are required if test result is marked as "Blocked"</description>
        <errorConditionFormula>AND(
ISPICKVAL( Test_Result_Status__c , "Blocked"),
Comments__c = "")</errorConditionFormula>
        <errorMessage>Comments required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Failure_Criticality_required_for_fails</fullName>
        <active>false</active>
        <description>Failure Criticality required for failed Test Results</description>
        <errorConditionFormula>OR(TEXT( Test_Result_Status__c ) = "Failed",TEXT( Test_Result_Status__c ) = "Passed, with Exception") &amp;&amp; ISBLANK(TEXT( Failure_Criticality__c ))</errorConditionFormula>
        <errorMessage>You must state failure criticality for failed test results</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Failure_Reason_required_for_fails</fullName>
        <active>true</active>
        <description>Failure Reason is required for failed test results</description>
        <errorConditionFormula>TEXT( Test_Result_Status__c ) != "Passed" &amp;&amp; ISBLANK(TEXT( Failure_Reason__c ))</errorConditionFormula>
        <errorMessage>A failure reason is required for failed test results</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Steps_to_reproduce_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
OR(
ISBLANK(Test_Record_URL__c),
ISBLANK(Steps_to_Reproduce__c )),
OR(
ISPICKVAL(Test_Result_Status__c , "Failed"),
ISPICKVAL(Test_Result_Status__c , "Passed, with Exception")
))</errorConditionFormula>
        <errorMessage>Steps to Reproduce and URL for the Test Record that Failed or Passed with Exception is required</errorMessage>
    </validationRules>
</CustomObject>
