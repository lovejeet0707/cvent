        <active>true</active>
        <description>#12601601</description>
        <errorConditionFormula>AND(
NOT(ISBLANK(Parent.ContactId )),
Parent.RecordType.DeveloperName = 'Finance_Customer_Support',
CONTAINS(ToAddress , Parent.Contact.Email),
NOT(CONTAINS($User.Username, 'deployment@cvent.com'))
  
)</errorConditionFormula>
        <errorDisplayField>ToAddress</errorDisplayField>
        <errorMessage>Please remove the Contact from To Address</errorMessage>
