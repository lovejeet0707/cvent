    <validationRules>
        <fullName>Exception_Comments_added_by_Approver</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  Exception_approved_by_email__c &lt;&gt;   $User.Email  ,
ISCHANGED(Exception_comment__c))</errorConditionFormula>
        <errorDisplayField>Exception_comment__c</errorDisplayField>
        <errorMessage>Exception Comments needs to be added by the Exception Approver.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Execution_Approved_Section_to_be_filled</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(CMR_Status__c = '3C-Execution Approved'), 
OR(ISCHANGED(Requirement_sent_to_CRM_team__c),
ISCHANGED(CRM_SPOC_for_the_Request__c),
ISCHANGED(Support_Ops_SPOC__c),
ISCHANGED(Actual_Release_Tag_date__c),
ISCHANGED(Release_tag_date_changed__c),
ISCHANGED(Release_tag_date_changed_reason__c),
ISCHANGED(Actual_Ready_for_UAT_date_from_CRM__c),
ISCHANGED(UAT_communication_sent_to_the_requester__c),
ISCHANGED(Actual_UAT_Sign_off_received__c)
))</errorConditionFormula>
        <errorMessage>You are not allowed to make the updates before the status is "Execution Approved"</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Execution_Approved_form_to_be_filled</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISCHANGED(Feasibility_Done_date__c),
 RecordTypeId ='0121h0000000Apx',
NOT(ISBLANK(Requirement_discussion_meeting__c)),
 $User.Id &lt;&gt;  Requirement_Gathering_Form__r.CreatedById )</errorConditionFormula>
        <errorMessage>The Feasibility done date will be updated by the user who gathered the requirement.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Red_flag_comments_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND( Red_flag_raised__c , 
ISBLANK( Redflag_comments__c ))</errorConditionFormula>
        <errorMessage>Red Flag Comments is required.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Release_Deffered_Reason_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND( UAT_done_on_time__c, 
ISBLANK( Release_deferred__c ))</errorConditionFormula>
        <errorMessage>UAT Timelines not met, please fill the appropriate release deferred reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Release_Tag_Changed_Reason</fullName>
        <active>true</active>
        <errorConditionFormula>AND( NOT(ISBLANK(Release_tag_date_changed__c)) ,
ISBLANK(Release_tag_date_changed_reason__c)
)</errorConditionFormula>
        <errorMessage>Please fill the Release tag date changed reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Requirement_Freeze_checkbox_update</fullName>
        <active>true</active>
        <description>Requirement Freeze by requester checkbox can be changed by Creator, Anuj Abrol, Sushil and David</description>
        <errorConditionFormula>AND(ISCHANGED( Requirement_freeze_by_requester__c ),
 $User.Id &lt;&gt; "00500000007RwJC",
$User.Id &lt;&gt; "00500000006y5WD",
$User.Id &lt;&gt; "00500000007C9CM",
$User.Id &lt;&gt; CreatedById
)</errorConditionFormula>
        <errorDisplayField>Requirement_freeze_by_requester__c</errorDisplayField>
        <errorMessage>You are not authorized to update the checkbox.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Requirement_discussion_meeting_update</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
 ISCHANGED(Requirement_discussion_meeting__c),
 $User.Id &lt;&gt; "00500000007RwJC",
 $User.Id &lt;&gt; "00500000007C9CM",
 $User.Id &lt;&gt;"00500000006y5WD"
)</errorConditionFormula>
        <errorDisplayField>Requirement_discussion_meeting__c</errorDisplayField>
        <errorMessage>You are not authorized to update the Requirement Discussion Meeting.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Requirement_freeze_by_requester_update</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISCHANGED(Requirement_freeze_by_requester__c),
Requirement_freeze_by_requester__c,
 $User.Id &lt;&gt; CreatedById )</errorConditionFormula>
        <errorMessage>You are not authorized to update the Requirement Freeze checkbox.</errorMessage>
    </validationRules>
