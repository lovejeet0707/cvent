    <validationRules>
        <fullName>Associate_Escalation_with_Case</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(Do_we_need_to_associate_it_with_case__c,"Yes"),
ISBLANK(Case__c),
NOT(CONTAINS($User.Username,"deploymentuser@cvent.com")) , 
$Setup.rh2__PS_Settings__c.rh2__Run_Validation_Rule__c /*Added to bypass rollup helper updates */ 
)</errorConditionFormula>
        <errorDisplayField>Case__c</errorDisplayField>
        <errorMessage>Please associate the escalation with case.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Escalation_Cause_cannot_be_updated</fullName>
        <active>true</active>
        <errorConditionFormula>AND(CONTAINS($User.Profile_Name__c,"Marketing"), 
ISCHANGED(Escalation_Cause__c), 
NOT(
ISPICKVAL(Escalation_Cause__c,"Process Breakdown")),
NOT(ISPICKVAL(Escalation_Cause__c,"Data Issue (Marketing)")),
NOT(ISPICKVAL(Escalation_Cause__c,"Coordination"))
)</errorConditionFormula>
        <errorDisplayField>Escalation_Cause__c</errorDisplayField>
        <errorMessage>You cannot select the Escalation cause other than Coordination/Process Breakdown/Data Issue (Marketing).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Escalation_Resolved_Update</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISPICKVAL(Escalation_Status__c,"Closed")),
ISCHANGED(Escalation_Resolved__c),
Escalation_Resolved__c)</errorConditionFormula>
        <errorDisplayField>Escalation_Resolved__c</errorDisplayField>
        <errorMessage>You can update this checkbox once the escalation status is Closed.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Please_fill_the_required_fields</fullName>
        <active>true</active>
        <description>validation for the following fields that they can not be left blank:
a.	Escalation Category (all record types and views)
b.	Escalation Description (all record types and views)
c.	Escalation Cause (only in Sops record type and view)</description>
        <errorConditionFormula>OR(
Escalation_Description__c="",
ISPICKVAL(Escalation_Category__c,""),
AND(CONTAINS($User.Profile_Name__c,"Sops") ,
ISPICKVAL(Escalation_Cause__c,""))
)</errorConditionFormula>
        <errorMessage>Please fill the Escalation Cause/Escalation Description/Escalation Category</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Resolution_cannot_be_blank</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISCHANGED(Escalation_Status__c), 
ISPICKVAL(Escalation_Status__c,"Closed"), 
OR(
ISBLANK(Resolution_Date__c), 
ISBLANK(Resolution__c), 
ISBLANK(TEXT(Escalation_Cause__c))),
NOT(CONTAINS($User.Username,"deploymentuser@cvent.com")) ,
$Setup.rh2__PS_Settings__c.rh2__Run_Validation_Rule__c /*Added to bypass rollup helper updates */ 
)</errorConditionFormula>
        <errorMessage>Please fill the Resolution/Resolution Date and Escalation Cause.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sales_Ops_team_should_fill_the_fields</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
ISCHANGED(Escalation_Status__c),
ISPICKVAL(Escalation_Status__c,"Closed"), 
OR(
ISPICKVAL(Department_Responsible__c,"Sales"),
ISPICKVAL(Department_Responsible__c,"Marketing")
),
OR(
ISBLANK(Escalated_By__c), 
ISBLANK(Person_Responsible__c),
ISBlank(TEXT(Escalation_Cause__c)), 
ISBLANK(Resolution__c) , 
ISBLANK(Resolution_Date__c)
),
NOT(CONTAINS($User.Username,"deploymentuser@cvent.com")) 
)</errorConditionFormula>
        <errorMessage>Please update the Escalation Status to Closed and update the fields - Escalated By/Person Responsible/Escalation Cause/Resolution/Resolution Date.</errorMessage>
    </validationRules>
</CustomObject>
