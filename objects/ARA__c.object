        <fullName>CRC_Support_Field</fullName>
        <active>true</active>
        <description>No one except Erin Phelps, Kelly Morrissey, Tyler Stevens, Rachael Macaseib, Pearson Weems should be able to modify CRC Support field value</description>
        <errorConditionFormula>AND(

OR(RecordTypeId='012o00000012ZYw', /*New ARA*/
RecordTypeId='012o00000012ZYx'), /*One-Off Escalation*/ 

ISCHANGED(CRC_Support__c),
NOT(
OR(
$User.Email = 'ephelps@cvent.com',
$User.Email = 'kmorrissey@cvent.com',
$User.Email = 'tstevens@cvent.com',
$User.Email = 'rmacasieb@cvent.com',
$User.Email = 'pweems@cvent.com'
)
),

NOT(CONTAINS($User.Username,"deploymentuser@cvent.com")),
