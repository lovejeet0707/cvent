    <validationRules>
        <fullName>ARA_can_only_be_open</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(CONTAINS($User.Username,"deploymentuser@cvent.com")), 
ISNEW(),
NOT(ISPICKVAL(Status__c,"Open"))
)</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>ARA can be submitted only in Open status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>App_Support_for_Product_Performance</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(CONTAINS($User.Username,"deploymentuser@cvent.com")), 
 OR(ISPICKVAL(ARA_Category__c,"Product Performance"),
    ISPICKVAL(Secondary_ARA_Category__c,"Product Performance")),
 ISBLANK( Related_App_Support_Ticket_SF_Link__c ),
 NOT(ISCHANGED(Status__c))
)</errorConditionFormula>
        <errorDisplayField>Related_App_Support_Ticket_SF_Link__c</errorDisplayField>
        <errorMessage>Please enter Related App Support Ticket SF Link</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>AssignedTo_cannot_be_changed</fullName>
        <active>true</active>
        <errorConditionFormula>AND(

NOT(CONTAINS($User.Username,"deploymentuser@cvent.com")),

ISCHANGED(Assigned_To__c),
 
OR(
RecordTypeId = "012o00000012ZYw",/*New ARA*/
RecordTypeId = "012o00000012ZYx"),/*One-Off Escalation*/

AND(
$User.Username &lt;&gt; "ephelps@cvent.com" , 
$User.Username &lt;&gt; "kmorrissey@cvent.com",
$User.Username &lt;&gt; "tstevens@cvent.com",
$User.Username &lt;&gt; "rmacasieb@cvent.com")

)</errorConditionFormula>
        <errorMessage>You are not authorized to Assign this case</errorMessage>
    </validationRules>
